{
  "name": "actc",
  "type": "module",
  "version": "1.0.0",
  "description": "Convert AC from Jira to a skeleton test case. Currently only support playwright",
  "main": "./lib/main.js",
  "bin": {
    "actc": "bin/script.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "convert": "node ./lib/main.js",
    "dev": "npm run build && npm run convert",
    "prepublish": "npm run build"
  },
  "author": "yazed jamal",
  "license": "MIT",
  "dependencies": {
    "config": "^3.3.9",
    "fast-xml-parser": "^4.2.7",
    "jsdom": "^22.1.0"
  },
  "devDependencies": {
    "@types/config": "^3.3.1",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.6.2",
    "typescript": "^5.2.2"
  }
}
